write("Using the for loop:");
for (let i=0; i<10; i+=1) {
 write("Current value of i: ", i);
}

write("Using the while loop:");
let j = 0;
while (j < 5) {
 write("Current value of j: ", j);
 j = j + 1;
}

write("Using the do-while loop:");
let k = 0;
do {
 write("Current value of k: ", k);
 k = k + 1;
} while (k < 5);